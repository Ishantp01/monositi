{
  "name": "monositi-backend",
  "version": "1.0.0",
  "description": "Backend API for Monositi - Property rental and service provider platform",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "echo 'No build step required'",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "db:seed": "node src/scripts/seed.js",
    "db:migrate": "echo 'No migrations needed'",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "production": "NODE_ENV=production npm start"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb",
    "jwt",
    "property-rental",
    "service-provider",
    "api"
  ],
  "author": "Monositi Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "redis": "^4.6.11",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "twilio": "^4.19.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "globals": "^13.24.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/monositi-backend.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/monositi-backend/issues"
  },
  "homepage": "https://github.com/your-org/monositi-backend#readme"
}
